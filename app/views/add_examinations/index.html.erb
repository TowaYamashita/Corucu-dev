<p id="notice"><%= notice %></p>

<h1>Add Examinations</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Subject</th>
      <th>Place</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_examinations.each do |add_examination| %>
      <tr>
        <td><%= add_examination.category %></td>
        <td><%= add_examination.subject %></td>
        <td><%= add_examination.place %></td>
        <td><%= add_examination.comment %></td>
        <td><%= link_to 'Show', add_examination %></td>
        <td><%= link_to 'Edit', edit_add_examination_path(add_examination) %></td>
        <td><%= link_to 'Destroy', add_examination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add Examination', new_add_examination_path %>
